    "graphql-tag": "2.12.6",
    "lodash-es": "4.17.21",
    "markdown-it": "14.1.0",
    "mitt": "3.0.1",
    "nprogress": "1.0.0-1",
    "preact": "10.25.4",
    "simple-icons": "2.19.0",
    "subscriptions-transport-ws": "0.11.0",
    "svg-pan-zoom": "3.6.2",
